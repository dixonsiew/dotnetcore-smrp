@model dynamic
{
    "name": "@Model.Name",
    "address": "@Model.Addr",
    "data": [
        @for (int i = 0; i < Model.List.Count; i++)
        {
            var x = Model.List[i];
            @:{
                @:"name": "@x.Name"
            @:}
            if (i < Model.List.Count - 1)
            {
                @:,
            }
        }
    ]
}